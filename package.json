{
  "name": "astra-mendacium",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run lingui && vite --host",
    "build": "bun run lingui && tsc -b && vite build",
    "lint:eslint": "eslint .",
    "preview": "vite preview",
    "fmt": "biome format ** .devcontainer .vscode",
    "fmt:fix": "biome format --write ** .devcontainer .vscode",
    "lint": "biome lint ** .devcontainer .vscode",
    "lint:fix": "biome lint --write ** .devcontainer .vscode",
    "lint:fix:unsafe": "biome lint --write --unsafe ** .devcontainer .vscode",
    "check": "biome check ** .devcontainer .vscode",
    "check:fix": "biome check --write ** .devcontainer .vscode",
    "check:fix:unsafe": "biome check --write --unsafe ** .devcontainer .vscode",
    "lingui": "bun run lingui:extract && bun run lingui:compile",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@lingui/core": "^5.8.0",
    "@lingui/react": "^5.8.0",
    "@tailwindcss/vite": "^4.1.18",
    "bun-plugin-tailwind": "^0.1.2",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.71.1",
    "react-router": "^7.12.0",
    "tailwindcss": "^4.1.11",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@eslint/js": "^9.39.1",
    "@lingui/swc-plugin": "^5.10.1",
    "@lingui/vite-plugin": "^5.8.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
