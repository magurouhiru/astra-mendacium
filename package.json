{
  "name": "astra-mendacium",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint:eslint": "eslint .",
    "preview": "vite preview",
    "fmt": "bunx --bun biome format ** .devcontainer .vscode",
    "fmt:fix": "bunx --bun biome format --write ** .devcontainer .vscode",
    "lint": "bunx --bun biome lint ** .devcontainer .vscode",
    "lint:fix": "bunx --bun biome lint --write ** .devcontainer .vscode",
    "lint:fix:unsafe": "bunx --bun biome lint --write --unsafe ** .devcontainer .vscode",
    "check": "bunx --bun biome check ** .devcontainer .vscode",
    "check:fix": "bunx --bun biome check --write ** .devcontainer .vscode",
    "check:fix:unsafe": "bunx --bun biome check --write --unsafe ** .devcontainer .vscode"
  },
  "dependencies": {
    "@lingui/react": "^5.8.0",
    "@tailwindcss/vite": "^4.1.18",
    "bun-plugin-tailwind": "^0.1.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.12.0",
    "tailwindcss": "^4.1.11",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@eslint/js": "^9.39.1",
    "@lingui/swc-plugin": "^5.10.1",
    "@lingui/vite-plugin": "^5.8.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
